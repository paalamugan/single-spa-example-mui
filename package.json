  "types": "dist/single-spa-example-react-mui.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paalamugan/single-spa-example-react-mui.git"
  },
  "keywords": [
    "single-spa",
    "single-spa-react",
    "mui",
    "react",
    "material UI"
  ],
  "author": "Paalamugan",
  "bugs": {
