{
  "name": "@single-spa-example/react-mui",
  "version": "1.0.0",
  "description": "MUI theme Common components for the single spa application",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --port 9002",
    "start:standalone": "webpack serve --env standalone",
